public class QuizCard {

    public QuizCard() { }

    public QuizCard(String q, String a) {

        setQuestion() {

            question = q;

        }

        setAnswer() {

            answer = a;

        }

    }

    private String question;

    private String answer;

    public String getQuestion() {

        return question;

    }

    public String getAnswer() {

        return answer;

    }

}
